{
   "name": "vsc-script",
   "displayName": "vsc-script",
   "description": "Create your own scripts on the fly and run them in vscode.",
   "publisher": "alfnielsen",
   "icon": "vsc-base-icon.png",
   "repository": {
      "type": "git",
      "url": "https://github.com/alfnielsen/vsc-base"
   },
   "homepage": "https://vsc-base.org",
   "version": "0.2.0",
   "engines": {
      "vscode": "^1.32.0"
   },
   "categories": [
      "Other"
   ],
   "activationEvents": [
      "onCommand:extension.vscScript"
   ],
   "main": "./dist/extension",
   "contributes": {
      "menus": {
         "explorer/context": [
            {
               "command": "extension.vscScript",
               "group": "1_modification",
               "when": "explorerViewletVisible"
            }
         ]
      },
      "commands": [
         {
            "command": "extension.vscScript",
            "title": "vsc Script"
         }
      ]
   },
   "scripts": {
      "vscode:prepublish": "webpack --mode production",
      "compile": "webpack --mode none",
      "watch": "webpack --mode none --watch",
      "test-compile": "tsc -p ./"
   },
   "devDependencies": {
      "@types/fs-extra": "^5.0.5",
      "@types/mocha": "^2.2.42",
      "@types/node": "^10.12.21",
      "ts-loader": "^5.3.3",
      "tslint": "^5.12.1",
      "vscode": "^1.1.28",
      "webpack": "^4.29.6",
      "webpack-cli": "^3.3.0"
   },
   "dependencies": {
      "fs-extra": "^7.0.1",
      "typescript": "^3.3.1",
      "vsc-base": "^0.3.1"
   }
}
