{
  "name": "vsc-script",
  "displayName": "vsc-script",
  "description": "Create your own scripts on the fly and run them in vscode.",
  "publisher": "alfnielsen",
  "icon": "vsc-base-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/alfnielsen/vsc-base"
  },
  "homepage": "https://vsc-base.org",
  "version": "0.5.2",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "extension.vscScript",
          "group": "1_modification",
          "when": "explorerViewletVisible"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.vscScript",
        "title": "vsc Script"
      },
      {
        "command": "extension.vscScriptOnSave",
        "title": "vsc Script (Open document)"
      }
    ],
    "keybindings": [
      {
        "command": "extension.vscScriptOnSave",
        "key": "ctrl+alt+o",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.1.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "ts-loader": "^6.0.0",
    "tslint": "^5.16.0",
    "vscode": "^1.1.34",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@types/child-process-promise": "^2.2.1",
    "@types/vscode": "^1.33.0",
    "child-process-promise": "^2.2.1",
    "fs-extra": "^7.0.1",
    "typescript": "^3.4.5",
    "vsc-base": "^0.8.30"
  }
}
